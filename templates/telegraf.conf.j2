[global_tags]
[agent]
  interval = "10s"
  round_interval = true
  metric_batch_size = 1000
  metric_buffer_limit = 10000
  collection_jitter = "0s"
  flush_interval = "10s"
  flush_jitter = "0s"
  precision = "0s"
  omit_hostname = true
[[outputs.kafka]]
  brokers = ["localhost:9092"]
  topic = "ansiblenetwork"
  data_format = "json"
[[inputs.cisco_telemetry_mdt]]
 transport = "grpc"
 service_address = ":{{ telegraf_port }}"
